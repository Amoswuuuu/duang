[

  {
    "key": "normal-list",
    "title": "配置 - 列表 - 最简单的列表",
    "fields": [
      { "key": "id", "title": "ID" },
      { "key": "type", "title": "类型" },
      { "key": "title", "title": "名称" },
      { "key": "description", "title": "描述" }
    ]
  },

  {
    "key": "components-list",
    "title": "配置 - 列表 - 列表中使用组件",
    "fields": [
      { "key": "id", "title": "ID" },
      { "key": "title", "title": "<font color=red>加特技</font>" },
      { "key": "value", "title": "排序", "sortable": true },
      {
        "key": "description", "title": "悬停提示控件", "component": "TextTip",
        "args": { "tip": "无提示文字", "text": "悬停显示描述" }
      },
      { "key": "img", "title": "图片控件", "component": "Image" }
    ]
  },

  {
    "key": "complex-list",
    "title": "配置 - 列表 - 相当复杂的列表",
    "gentleRefreshing": true,
    "listSelector": true,
    "actions": [
      { "title": "www.ele.me", "method": "open", "href": "https://www.ele.me" },
      { "title": "The-Action", "method": "post", "api": "theAction" }
    ],
    "caption": [
      {
        "component": "HTML",
        "args": { "html": "数据加载时间 {value}", "@defaultValue": "locale-datetime" }
      }
    ],
    "fields": [
      { "key": "id", "title": "ID" },
      { "key": "type", "title": "类型" },
      { "key": "title", "title": "名称" },
      { "key": "description", "title": "描述" }
    ],
    "headers": [
      { "component": "AutoRefresh", "args": { "interval": 3000 } }
    ],
    "operations": [
      { "title": "批量删除", "method": "delete", "query": true }
    ]
  },

  {
    "listSelector": true,
    "key": "the-list-data?2",
    "title": "配置 - 列表 - 可全选",
    "actions": [
      { "title": "www.ele.me", "method": "open", "href": "https://www.ele.me" },
      { "title": "The-Action", "method": "post", "api": "theAction" }
    ],
    "fields": [
      {
        "key": "id",
        "title": "ID",
        "sortable": true
      },
      {
        "key": "a", "title": "<font color=red>A</font>",
        "component": "TextTip"
      },
      {
        "key": "img", "title": "图片",
        "component": "Image"
      },
      {
        "key": "c",
        "component": "QRCode",
        "args": {
          "defaultValue": "hehe"
        },
        "title": "QRCode"
      }
    ],
    "operations": [
      { "title": "批量删除", "method": "delete", "query": true }
    ]
  },

  {
    "key": "the-list-data?3",
    "title": "配置 - 列表 - 默认筛选条件",
    "where": { "f3": "a" },
    "filters": [
      {
        "key": "f1",
        "component": "String",
        "args": { "width": 100 },
        "title": "Input - 100",
        "description": "描述"
      },
      {
        "key": "f2",
        "component": "String",
        "optional": true,
        "args": { "width": 150 },
        "title": "Input - 150",
        "description": "描述"
      },
      {
        "key": "f3",
        "component": "Radio",
        "optional": true,
        "args": {
          "options": { "a": "1", "b": "2" },
          "defaultValue": "a"
        },
        "title": "Radio"
      },
      {
        "key": "f4",
        "component": "Checkbox",
        "args": {
          "options": { "a": "一个很长很长的描述", "b": "一个很长很长的描述", "c": "一个很长很长的描述", "d": "一个很长很长的描述" },
          "defaultValue": [ "a", "b" ]
        },
        "title": "Radio"
      }
    ],
    "actions": [
      { "title": "www.ele.me", "method": "open", "href": "https://www.ele.me" },
      { "title": "The-Action", "method": "post", "api": "theAction" }
    ],
    "fields": [
      {
        "key": "id",
        "title": "ID",
        "sortable": true
      },
      {
        "key": "a", "title": "<font color=red>A</font>",
        "component": "TextTip"
      },
      {
        "key": "img", "title": "图片",
        "component": "Image"
      },
      {
        "key": "c",
        "component": "QRCode",
        "args": {
          "defaultValue": "hehe"
        },
        "title": "QRCode"
      }
    ],
    "operations": [
      { "title": "批量删除", "method": "delete", "query": true }
    ]
  },

